	Journal = {Evolution and Human Behavior},
	Keywords = {Cooperation, Eyes, Field experiment, Prosocial behaviour},
	Month = may,
	Number = {3},
	Pages = {172--178},
	Shorttitle = {Effects of eye images on everyday cooperative behavior},
	Title = {Effects of eye images on everyday cooperative behavior: a field experiment},
