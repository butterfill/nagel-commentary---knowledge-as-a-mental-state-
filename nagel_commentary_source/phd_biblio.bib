	Doi = {10.1016/j.neuropsychologia.2010.07.037},
	Issn = {0028-3932},
	Journal = {Neuropsychologia},
	Keywords = {Connectivity, Observation, Parietal cortex, premotor cortex, Transcranial Magnetic Stimulation},
	Month = oct,
	Number = {12},
	Pages = {3513--3520},
	Title = {Resonance of cortico--cortical connections of the motor system with the observation of goal directed grasping movements},
	Url = {http://www.sciencedirect.com/science/article/pii/S0028393210003441},
	Volume = {48},
