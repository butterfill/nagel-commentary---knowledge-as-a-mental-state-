	Journal = {Mind \& Language},
	Month = nov,
	Number = {5},
	Pages = {475--513},
	Shorttitle = {Objects of Desire, Thought, and Reality},
	Title = {Objects of Desire, Thought, and Reality: Problems of Anchoring Discourse Referents in Development},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1468-0017.2007.00317.x/abstract},
	Volume = {22},
	Year = {2007},
	Bdsk-Url-1 = {http://onlinelibrary.wiley.com/doi/10.1111/j.1468-0017.2007.00317.x/abstract},
