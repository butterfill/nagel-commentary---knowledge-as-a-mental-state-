	Issn = {1943-3921},
	Journal = {Attention, Perception, \& Psychophysics},
	Keywords = {Behavioral Science},
	Number = {2},
	Pages = {201--211},
	Title = {Visual perception of biological motion and a model for its analysis},
	Url = {http://www.springerlink.com/content/f07t232637745786/abstract/},
	Volume = {14},
	Year = {1973},
