	Journal = {Trends in Cognitive Sciences},
	Keywords = {Animal reasoning, Counting, Non-verbal reasoning, Numerical processing, Scalar variability, Subitizing},
	Month = feb,
	Number = {2},
	Pages = {59--65},
	Shorttitle = {Non-verbal numerical cognition},
	Title = {Non-verbal numerical cognition: from reals to integers},
