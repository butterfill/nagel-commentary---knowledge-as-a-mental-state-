	Issn = {0028-3932},
	Journal = {Neuropsychologia},
	Keywords = {action, Bodily self, Mirror neurons, Sense of agency, Sense of ownership},
	Month = feb,
	Number = {3},
	Pages = {746--755},
	Title = {The bodily self as power for action},
