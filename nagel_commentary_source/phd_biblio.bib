	Issn = {0270-6474, 1529-2401},
	Journal = {The Journal of Neuroscience},
	Keywords = {Action observation, Biological motion, frontal, functional {MRI}, Motion, premotor cortex},
	Month = jul,
	Number = {27},
	Pages = {6181--6188},
	Title = {{Point-Light} Biological Motion Perception Activates Human Premotor Cortex},
