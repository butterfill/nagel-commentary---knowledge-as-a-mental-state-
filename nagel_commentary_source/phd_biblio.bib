	Issn = {1467-8624},
	Journal = {Child Development},
	Number = {6},
	Pages = {1227--1246},
	Shorttitle = {Object Permanence in Young Infants},
	Title = {Object Permanence in Young Infants: Further Evidence},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1467-8624.1991.tb01602.x/abstract},
	Volume = {62},
